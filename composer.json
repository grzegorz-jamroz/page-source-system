{
  "name": "grzegorz-jamroz/page-source-system",
  "description": "Library generate html readable by seo robots and api json for React page.",
  "require": {
    "php": ">=8",
    "doctrine/collections": "^1.6",
    "grzegorz-jamroz/html-creator": "dev-master",
    "grzegorz-jamroz/simple-storage-system": "dev-master",
    "grzegorz-jamroz/plain-data-transformer": "dev-master",
    "grzegorz-jamroz/page-source-components": "dev-master",
    "grzegorz-jamroz/php-common": "dev-master",
    "ramsey/uuid": "^4.2",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "PageSourceSystem\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^0.12.79"
  }
}
